{
  "id": "leeebo-detail",
  "baseUrl": "http://www.leeebo.com",
  "pages": [
    "/",
    "/vod-type-id-1-pg-1.html",
    "/vod-type-id-2-pg-1.html",
    "/vod-type-id-3-pg-1.html",
    "/vod-type-id-16-pg-1.html",
    "",
    "/vod-type-id-20-pg-1.html",
    "/index.php?m=vod-search"
  ],
  "catalogSelector": {
    "cssQuery": "section.mod div.mod-bd div.numList",
    "tagSelector": {
      "cssQuery": "li a.bd",
      "titleSelector": {
        "analyzer": {
          "method": 0
        }
      },
      "urlSelector": {
        "analyzer": {
          "method": 1,
          "args": [
            "href"
          ],
          "format": "{baseUrl}%s"
        }
      }
    }
  },
  "dataSelector": {
    "attrSelectors": [
      {
        "label": "thumbnail",
        "global": true,
        "cssQuery": "section.page-hd img",
        "analyzer": {
          "method": 1,
          "args": [
            "src"
          ],
          "regexp": {
            "pattern": "^/",
            "format": "{baseUrl}%s"
          }
        }
      },
      {
        "label": "intro",
        "global": true,
        "cssQuery": "article.detail-con",
        "analyzer": {
          "method": 0
        }
      },
      {
        "label": "href",
        "global": true,
        "cssQuery": "section.page-btn a",
        "analyzer": {
          "method": 1,
          "args": [
            "href"
          ],
          "format": "{baseUrl}%s"
        }
      },
      {
        "label": "desc",
        "placeholder": "intro"
      }
    ],
    "tagSelector": {
      "global": true,
      "cssQuery": "section.page-bd div.desc_item",
      "titleSelector": {
        "analyzer": {
          "method": 0
        }
      }
    }
  }
}