{
  "id": "btcherry",
  "baseUrl": "http://www.btcherry.info",
  "pages": [
    "/",
    "/search?keyword="
  ],
  "searchSelector": {
    "url": "{baseUrl}{pages[1]}(keyword)",
    "headers": {
      "User-Agent": "Mozilla/5.0 (Windows NT 10.0; WOW64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/54.0.2840.99 Safari/537.36"
    },
    "cssQuery": "div.r a.link",
    "attrSelectors": [
      {
        "label": "title",
        "analyzer": {
          "method": 0
        }
      },
      {
        "label": "magnet",
        "preTreat": {
          "nextElementSibling": true
        },
        "cssQuery": "a",
        "analyzer": {
          "method": 1,
          "args": [
            "href"
          ]
        }
      },
      {
        "label": "size",
        "preTreat": {
          "nextElementSibling": true
        },
        "cssQuery": "span.prop_val",
        "filter": {
          "indexes": [
            1
          ]
        },
        "analyzer": {
          "method": 0
        }
      },
      {
        "label": "files",
        "preTreat": {
          "nextElementSibling": true
        },
        "cssQuery": "span.prop_val",
        "filter": {
          "indexes": [
            2
          ]
        },
        "analyzer": {
          "method": 0
        }
      },
      {
        "label": "addTime",
        "preTreat": {
          "nextElementSibling": true
        },
        "cssQuery": "span.prop_val",
        "filter": {
          "indexes": [
            0
          ]
        },
        "analyzer": {
          "method": 0
        }
      },
      {
        "label": "popularity",
        "content": ""
      }
    ],
    "nextPageSelector": {
      "content": "{baseUrl}{pages[1]}(keyword)&p={number}"
    }
  }
}