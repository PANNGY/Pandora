{
  "id": "javlib-tab",
  "baseUrl": "https://www.13vlib.com",
  "pages": [
    "/vl_update.php",
    "/vl_newrelease.php",
    "/vl_newentries.php",
    "/vl_mostwanted.php",
    "/vl_bestrated.php",
    "/vl_searchbyid.php?keyword=",
    "/genres.php"
  ],
  "areas": {
    "en": "en",
    "ja": "ja",
    "cn": "cn",
    "tw": "tw"
  },
  "tabSelector": {
    "url": "{baseUrl}/{area}{pages[3]}",
    "cssQuery": "div#leftmenu div.menul1 ul li a",
    "filter": {
      "indexes": [
        0,
        1,
        2,
        3,
        4
      ]
    },
    "titleSelector": {
      "analyzer": {
        "method": 0
      }
    },
    "urlSelector": {
      "analyzer": {
        "method": 1,
        "args": [
          "href"
        ],
        "format": "{baseUrl}/{area}/%s"
      }
    }
  },
  "catalogSelector": {
    "url": "{baseUrl}/{area}{pages[6]}",
    "cssQuery": "div#rightcolumn div.textbox",
    "titleSelector": {
      "cssQuery": "div.boxtitle",
      "analyzer": {
        "method": 0
      }
    },
    "tagSelector": {
      "cssQuery": "div.genreitem",
      "titleSelector": {
        "analyzer": {
          "method": 0
        }
      },
      "urlSelector": {
        "cssQuery": "a",
        "analyzer": {
          "method": 1,
          "args": [
            "href"
          ],
          "format": "{baseUrl}/{area}/%s"
        }
      }
    }
  },
  "dataSelector": {
    "url": "{baseUrl}/{area}{pages[3]}",
    "cssQuery": "div.videothumblist div.video",
    "attrSelectors": [
      {
        "label": "id",
        "cssQuery": "a div.id",
        "analyzer": {
          "method": 0
        }
      },
      {
        "label": "title",
        "cssQuery": "a div.title",
        "analyzer": {
          "method": 0
        }
      },
      {
        "label": "thumbnail",
        "cssQuery": "a img",
        "analyzer": {
          "method": 1,
          "args": [
            "src"
          ]
        }
      },
      {
        "label": "href",
        "cssQuery": "a",
        "analyzer": {
          "method": 1,
          "args": [
            "href"
          ],
          "format": "{baseUrl}/{area}/%s"
        }
      }
    ],
    "nextPageSelector": {
      "cssQuery": "div.page_selector a.next",
      "analyzer": {
        "method": 1,
        "args": [
          "href"
        ],
        "format": "{baseUrl}%s"
      }
    }
  },
  "searchSelector": {
    "url": "{baseUrl}/{area}{pages[5]}{keyword}",
    "cssQuery": "div.videothumblist div.video",
    "attrSelectors": [
      {
        "label": "id",
        "cssQuery": "a div.id",
        "analyzer": {
          "method": 0
        }
      },
      {
        "label": "title",
        "cssQuery": "a div.title",
        "analyzer": {
          "method": 0
        }
      },
      {
        "label": "thumbnail",
        "cssQuery": "a img",
        "analyzer": {
          "method": 1,
          "args": [
            "src"
          ]
        }
      },
      {
        "label": "href",
        "cssQuery": "a",
        "analyzer": {
          "method": 1,
          "args": [
            "href"
          ],
          "format": "{baseUrl}/{area}/%s"
        }
      }
    ],
    "nextPageSelector": {
      "cssQuery": "div.page_selector a.next",
      "analyzer": {
        "method": 1,
        "args": [
          "href"
        ],
        "format": "{baseUrl}%s",
        "regexp": {
          "pattern": "vl_update.php",
          "replace": "/{area}/vl_update.php"
        }
      }
    },
    "reserveSelector": {
      "cssQuery": "div.previewthumbs img",
      "attrSelectors": [
        {
          "label": "cover",
          "global": true,
          "cssQuery": "img#video_jacket_img",
          "analyzer": {
            "method": 1,
            "args": [
              "src"
            ]
          }
        },
        {
          "label": "thumbnail",
          "placeholder": "cover",
          "analyzer": {
            "method": 1,
            "args": [
              "src"
            ]
          }
        }
      ],
      "tagSelector": {
        "global": true,
        "cssQuery": "div#video_info div[id^=video_] span a",
        "titleSelector": {
          "analyzer": {
            "method": 0
          }
        },
        "urlSelector": {
          "analyzer": {
            "method": 1,
            "args": [
              "href"
            ],
            "format": "{baseUrl}/{area}/%s"
          }
        }
      }
    }
  }
}