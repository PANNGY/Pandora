    "attrSelectors": [
      {
        "label": "thumbnail",
        "cssQuery": "img",
        "analyzer": {
          "method": 1,
          "args": [
            "delay",
            "src"
          ]
        }
      }
    ],
    "tagSelector": {
      "global": true,
      "cssQuery": "span.tag a",
      "titleSelector": {
        "analyzer": {
          "method": 0
        }
      },
      "urlSelector": {
        "analyzer": {
          "method": 1,
          "args": [
            "href"
          ],
          "format": "{baseUrl}/%s"
        }
      }
    }
  }
}