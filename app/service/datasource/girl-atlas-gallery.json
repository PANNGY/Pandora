{
  "baseUrl": "https://www.girl-atlas.com",
  "pages": [
    "/",
    "/index1"
  ],
  "catalogSelector": {
    "url": "{baseUrl}/",
    "cssQuery": "div.sidebar div.mb-10",
    "titleSelector": {
      "analyzer": {
        "method": 0
      }
    },
    "tagSelector": {
      "cssQuery": "div.tag",
