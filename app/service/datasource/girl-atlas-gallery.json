{
  "baseUrl": "https://www.girl-atlas.com",
  "pages": [
    "/",
    "/index1"
  ],
  "dataSelector": {
    "headers": {
      "User-Agent": "Mozilla/5.0 (Windows NT 10.0; WOW64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/54.0.2840.99 Safari/537.36"
    },
    "cssQuery": "li.slide",
    "filter": {
      "notQuery": ".best"
    },
    "attrSelectors": [
      {
        "label": "thumbnail",
        "cssQuery": "img",
        "analyzer": {
          "method": 1,
          "args": [
            "delay",
            "src"
          ]
        }
      }
    ],
    "tagSelector": {
      "global": true,
      "cssQuery": "span.tag a",
      "titleSelector": {
        "analyzer": {
          "method": 0
        }
      },
      "urlSelector": {
        "analyzer": {
          "method": 1,
          "args": [
            "href"
          ],
          "format": "{baseUrl}/%s"
        }
      }
    }
  }
}