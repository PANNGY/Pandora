{
  "id": "k8dy-home",
  "baseUrl": "http://www.k8dy.top",
  "pages": [
    "/",
    "/frim/index1.html",
    "/frim/index2.html",
    "/frim/index3.html",
    "/frim/index4.html",
    "/frim/index18.html",
    "/frim/index19.html",
    "/search.php?searchword="
  ],
  "dataSelector": {
    "headers": {
      "Accept": "*/*",
      "Accept-Encoding": "gzip, deflate, sdch",
      "Accept-Language": "en-US,en;q=0.8,zh-CN;q=0.6,zh;q=0.4,ja;q=0.2",
      "DNT": "1",
      "User-Agent": "Mozilla/5.0 (Macintosh; Intel Mac OS X 10_12_2) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/55.0.2883.87 Safari/537.36"
    },
    "groupSelector": {
      "cssQuery": "div.main div.index-area",
      "attrSelectors": [
        {
          "label": "type",
          "content": "group"
        },
        {
          "cssQuery": "h1.title",
          "label": "title",
          "analyzer": {
            "method": 2,
            "regexp": {
              "pattern": "[\u4e00-\u9fa5]+$"
            }
          }
        },
        {
          "label": "href",
          "cssQuery": "h1.title a",
          "filter": {
            "last": true
          },
          "analyzer": {
            "method": 1,
            "args": [
              "href"
            ],
            "format": "{baseUrl}%s"
          }
        }
      ]
    },
    "cssQuery": "li a.link-hover",
    "attrSelectors": [
      {
        "label": "type",
        "content": "data"
      },
      {
        "label": "href",
        "analyzer": {
          "method": 1,
          "args": [
            "href"
          ],
          "format": "{baseUrl}%s"
        }
      },
      {
        "label": "thumbnail",
        "cssQuery": "img",
        "analyzer": {
          "method": 1,
          "args": [
            "data-original"
          ],
          "regexp": {
            "pattern": "^/",
            "format": "{baseUrl}%s"
          }
        }
      },
      {
        "label": "hd",
        "cssQuery": "p.other",
        "analyzer": {
          "method": 0
        }
      },
      {
        "label": "title",
        "cssQuery": "span.lzbz p.name",
        "analyzer": {
          "method": 0
        }
      },
      {
        "label": "des",
        "cssQuery": "span.lzbz p.actor",
        "analyzer": {
          "method": 0
        }
      }
    ]
  }
}