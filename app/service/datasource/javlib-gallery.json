{
  "baseUrl": "http://www.j12lib.com",
  "pages": [
    "/vl_update.php",
    "/vl_newrelease.php",
    "/vl_newentries.php",
    "/vl_mostwanted.php",
    "/vl_bestrated.php",
    "/vl_searchbyid.php?keyword=",
    "/genres.php"
  ],
  "areas": {
    "en": "en",
    "ja": "ja",
    "cn": "cn",
    "tw": "tw"
  },
  "tabSelector": {
    "url": "{baseUrl}/{area}{pages[3]}",
    "cssQuery": "div#leftmenu div.menul1 ul li a",
    "filter": {
      "indexes": [
        0,
        1,
        2,
        3,
        4
      ]
    },
    "titleSelector": {
      "analyzer": {
        "method": 0
      }
    },
    "urlSelector": {
      "analyzer": {
        "method": 1,
        "args": [
          "href"
        ],
        "format": "{baseUrl}/{area}/%s"
      }
    }
  },
  "catalogSelector": {
    "url": "{baseUrl}/{area}{pages[6]}",
    "cssQuery": "div#rightcolumn div.textbox",
    "titleSelector": {
      "cssQuery": "div.boxtitle",
      "analyzer": {
        "method": 0
      }
    },
    "tagSelector": {
      "cssQuery": "div.genreitem",
      "titleSelector": {
        "analyzer": {
          "method": 0
        }
      },
      "urlSelector": {
        "cssQuery": "a",
        "analyzer": {
          "method": 1,
          "args": [
            "href"
          ],
          "format": "{baseUrl}/{area}/%s"
        }
      }
    }
  },
  "dataSelector": {
    "url": "http://www.j12lib.com/en/?v=javlikjn54",
    "cssQuery": "div.previewthumbs img",
    "attrSelectors": [
      {
        "label": "cover",
        "global": true,
        "cssQuery": "img#video_jacket_img",
        "analyzer": {
          "method": 1,
          "args": [
            "src"
          ]
        }
      },
      {
        "label": "thumbnail",
        "placeholder": "cover",
        "analyzer": {
          "method": 1,
          "args": [
            "src"
          ]
        }
      }
    ],
    "tagSelector": {
      "global": true,
      "cssQuery": "div#video_info div[id^=video_] span a",
      "titleSelector": {
        "analyzer": {
          "method": 0
        }
      },
      "urlSelector": {
        "analyzer": {
          "method": 1,
          "args": [
            "href"
          ],
          "format": "{baseUrl}/{area}/%s"
        }
      }
    }
  }
}