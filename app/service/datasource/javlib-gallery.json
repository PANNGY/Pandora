{
  "id": "javlib-gallery",
  "baseUrl": "https://www.ja14b.com",
  "pages": [
    "/vl_update.php",
    "/vl_newrelease.php",
    "/vl_newentries.php",
    "/vl_mostwanted.php",
    "/vl_bestrated.php",
    "/vl_searchbyid.php?keyword=",
    "/genres.php"
  ],
  "areas": {
    "en": "en",
    "ja": "ja",
    "cn": "cn",
    "tw": "tw"
  },
  "dataSelector": {
    "cssQuery": "div.previewthumbs img",
    "attrSelectors": [
      {
        "label": "cover",
        "global": true,
        "cssQuery": "img#video_jacket_img",
        "analyzer": {
          "method": 1,
          "args": [
            "src"
          ]
        }
      },
      {
        "label": "thumbnail",
        "placeholder": "cover",
        "analyzer": {
          "method": 1,
          "args": [
            "src"
          ]
        }
      }
    ],
    "tagSelector": {
      "global": true,
      "cssQuery": "div#video_info div[id^=video_] span a",
      "titleSelector": {
        "analyzer": {
          "method": 0
        }
      },
      "urlSelector": {
        "analyzer": {
          "method": 1,
          "args": [
            "href"
          ],
          "format": "{baseUrl}/{area}/%s"
        }
      }
    }
  }
}