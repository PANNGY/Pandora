{
  "baseUrl": "http://www.j12lib.com",
  "pages": [
    "/genres.php"
  ],
  "areas": {
    "en": "en",
    "ja": "ja",
    "cn": "cn",
    "tw": "tw"
  },
  "catalogSelector": {
    "url": "{baseUrl}/{area}{pages[0]}",
    "selector": "div#rightcolumn div.textbox",
    "titleSelector": {
      "selector": "div.boxtitle",
      "analyzer": {
        "method": 0
      }
    },
    "tagSelector": {
      "selector": "div.genreitem",
      "titleSelector": {
        "analyzer": {
          "method": 0
        }
      },
      "urlSelector": {
        "selector": "a",
        "analyzer": {
          "method": 1,
          "args": [
            "href"
          ],
          "format": "{baseUrl}%s"
        }
      }
    }
  }
}