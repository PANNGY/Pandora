{
  "baseUrl": "http://www.nanrencd.com",
  "pages": [
    "/",
    "/tuigirls",
    "/aiss",
    "/ddy",
    "/legbaby",
    "/mm",
    "/meinvmote",
    "/ugirls",
    "/tgod",
    "/?s="
  ],
  "dataSelector": {
    "url": "https://www.girl-atlas.com/album/57b2ef1a92d30218bee68b8a",
    "cssQuery": "div#post_content p img",
    "attrSelectors": [
      {
        "label": "thumbnail",
        "analyzer": {
          "method": 1,
          "args": [
            "src"
          ]
        }
      },
      {
        "label": "page-total",
        "global": true,
        "cssQuery": "div.pageList li.page-next a",
        "filter": {
          "last": true
        },
        "analyzer": {
          "method": 0
        }
      }
    ],
    "tagSelector": {
      "global": true,
      "cssQuery": "div.article_info span.info_category a",
      "titleSelector": {
        "analyzer": {
          "method": 0
        }
      },
      "urlSelector": {
        "analyzer": {
          "method": 1,
          "args": [
            "href"
          ]
        }
      }
    },
    "nextPageSelector": {
      "cssQuery": "div.pageList li.page-next a",
      "analyzer": {
        "method": 1,
        "args": [
          "href"
        ]
      },
      "autoLoad": true
    }
  }
}