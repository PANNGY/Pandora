{
  "baseUrl": "https://avso.pw",
  "pages": [
    "/",
    "/popular",
    "/actresses",
    "/genre",
    "/search/"
  ],
  "areas": {
    "en": "en",
    "ja": "ja",
    "cn": "cn",
    "tw": "tw"
  },
  "tabSelector": {
    "url": "{baseUrl}/{area}{pages[0]}",
    "cssQuery": "div#navbar ul.nav li a",
    "filter": {
      "indexes": [
        0,
        1
      ]
    },
    "titleSelector": {
      "analyzer": {
        "method": 0
      }
    },
    "urlSelector": {
      "analyzer": {
        "method": 1,
        "args": [
          "href"
        ]
      }
    }
  },
  "catalogSelector": {
    "url": "{baseUrl}/{area}{pages[3]}",
    "cssQuery": "div.container-fluid",
    "titleSelector": {
      "cssQuery": "h4",
      "analyzer": {
        "method": 0
      }
    },
    "tagSelector": {
      "cssQuery": "div.row.genre-box a",
      "titleSelector": {
        "analyzer": {
          "method": 0
        }
      },
      "urlSelector": {
        "analyzer": {
          "method": 1,
          "args": [
            "href"
          ]
        }
      }
    }
  },
  "dataSelector": {
    "url": "{baseUrl}/{area}{pages[0]}",
    "cssQuery": "div#waterfall div.item a.movie-box",
    "attrSelectors": [
      {
        "label": "id",
        "cssQuery": "div.photo-info date",
        "analyzer": {
          "method": 0,
          "regexp": {
            "pattern": "\\S+\\s$"
          }
        }
      },
      {
        "label": "title",
        "cssQuery": "div.photo-info span",
        "analyzer": {
          "method": 0
        }
      },
      {
        "label": "thumbnail",
        "cssQuery": "div.photo-frame img",
        "analyzer": {
          "method": 1,
          "args": [
            "src"
          ]
        }
      },
      {
        "label": "url",
        "analyzer": {
          "method": 1,
          "args": [
            "href"
          ]
        }
      }
    ],
    "nextPageSelector": {
      "cssQuery": "ul.pagination li a[name^=nextpage]",
      "analyzer": {
        "method": 1,
        "args": [
          "href"
        ],
        "format": "{baseUrl}%s"
      }
    }
  }
}